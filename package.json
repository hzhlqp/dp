    "babel-plugin-component": "^1.1.1",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^3.0.0",
    "element-ui": "2.13.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^1.18.2",
    "style-resources-loader": "^1.2.1",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10",
    "vuex-persistedstate": "^2.5.4"
